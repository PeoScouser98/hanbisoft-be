{
	"root": true,
	"plugins": [
		"prettier"
	],
	"extends": [
		"eslint:recommended",
		"eslint-config-prettier",
		"prettier"
	],
	"rules": {
		"no-unused-vars": "warn",
		"prettier/prettier": [
			"warn",
			{
				"printWidth": 120,
				"singleQuote": true,
				"tabWidth": 3,
				"useTabs": true,
				"endOfLine": "auto",
				"trailingComma": "none"
			}
		]
	}
}