{
	"version": 2,
	"buildCommand": "npm run vercel-build",
	"installCommand": "npm install",
	"outputDirectory": "dist",
	"builds": [
		{
			"src": "./package.json",
			"use": "@vercel/static-build",
			"config": { "distDir": "dist", "includeFiles": ["dist/app.js"] }
		},
		{
			"src": "app.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{ "handle": "filesystem" },
		{
			"src": "/(.*)",
			"dest": "app.js"
		}
	]
}
